generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  email    String @unique
  name     String
  imageUrl String

  created_at DateTime @default(now())

  @@map("users") //change name of table (model)
}
